alias usecode="export EDITOR="/usr/local/bin/code -w -n""
alias usesubl="export EDITOR="/usr/local/bin/subl -w -n""
alias rs="rsync -axHAWXS --numeric-ids --info=progress2"
alias scaffolder="npx scaff i --from-github ssh://git@gitea.eugeneleto.xyz/sea/scaffolder-templates.git"
alias vim="nvim"
alias typora="open -a '/Applications/Typora.app'"
alias lazygit="lazygit --ucd ~/.config/lazygit"
